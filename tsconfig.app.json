{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": [
    "env.d.ts",
    "src/**/*",
    "src/**/*.vue"
  ],
  "exclude": [
    "src/**/__tests__/*"
  ],
  "compilerOptions": {
    "moduleResolution": "Node",
    "composite": true,
    "baseUrl": ".",
    "types": ["element-plus/global","@vueuse/core"],
    "paths": {
      "@/*": [
        "./src/*"
      ]
    }
  }
}
